  , "build:testjs": "browserify client/mainTest.js -o client/indexTest.js"
  , "build:testjs-watch": "watchify client/mainTest.js -o client/indexTest.js"
  , "features": "cucumber-js features"
  , "features-wip": "cucumber-js --tags @wip features"
  , "selenium": "selenium-standalone start"
  , "server": "node server/index.js"
  , "server-watch": "nodemon --watch server server/index.js"
  }
